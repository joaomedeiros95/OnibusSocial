package com.joaoemedeiros.onibussocial.bd.model;

/**
 * The Class Localizacao.
 *
 * @author JoãoEduardo
 */
public class Localizacao {
	
	/** The id. */
	private int id;
	
	/** The id_onibus. */
	private int id_onibus;
	
	/** The localizacao. */
	private String localizacao;
	
	/** The ultima_atualizacao. */
	private String ultima_atualizacao;
	
	/**
	 * Instantiates a new localizacao.
	 */
	public Localizacao() {

	}
	
	/**
	 * Instantiates a new localizacao.
	 *
	 * @param id the id
	 * @param id_onibus the id_onibus
	 * @param localizacao the localizacao
	 * @param ultima_atualizacao the ultima_atualizacao
	 */
	public Localizacao(int id, int id_onibus, String localizacao, String ultima_atualizacao) {
		this.id = id;
		this.id_onibus = id_onibus;
		this.localizacao = localizacao;
		this.ultima_atualizacao = ultima_atualizacao;
	}
	
	/**
	 * Instantiates a new localizacao.
	 *
	 * @param id_onibus the id_onibus
	 * @param localizacao the localizacao
	 * @param ultima_atualizacao the ultima_atualizacao
	 */
	public Localizacao(int id_onibus, String localizacao, String ultima_atualizacao) {
		this.id_onibus = id_onibus;
		this.localizacao = localizacao;
		this.ultima_atualizacao = ultima_atualizacao;
	}

	/**
	 * Gets the id.
	 *
	 * @return the id
	 */
	public int getId() {
		return id;
	}

	/**
	 * Sets the id.
	 *
	 * @param id the new id
	 */
	public void setId(int id) {
		this.id = id;
	}

	/**
	 * Gets the localizacao.
	 *
	 * @return the localizacao
	 */
	public String getLocalizacao() {
		return localizacao;
	}

	/**
	 * Sets the localizacao.
	 *
	 * @param localizacao the new localizacao
	 */
	public void setLocalizacao(String localizacao) {
		this.localizacao = localizacao;
	}

	/**
	 * Gets the ultima_atualizacao.
	 *
	 * @return the ultima_atualizacao
	 */
	public String getUltima_atualizacao() {
		return ultima_atualizacao;
	}

	/**
	 * Sets the ultima_atualizacao.
	 *
	 * @param ultima_atualizacao the new ultima_atualizacao
	 */
	public void setUltima_atualizacao(String ultima_atualizacao) {
		this.ultima_atualizacao = ultima_atualizacao;
	}

	/**
	 * Gets the id_onibus.
	 *
	 * @return the id_onibus
	 */
	public int getId_onibus() {
		return id_onibus;
	}

	/**
	 * Sets the id_onibus.
	 *
	 * @param id_onibus the new id_onibus
	 */
	public void setId_onibus(int id_onibus) {
		this.id_onibus = id_onibus;
	}
	
}
