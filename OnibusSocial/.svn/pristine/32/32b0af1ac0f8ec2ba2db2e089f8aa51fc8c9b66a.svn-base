package com.joaoemedeiros.onibussocial.bd.model;

/**
 * The Class Tracker.
 */
public class Acompanhador {
	
	/** The id. */
	private int id;
	
	/** The id_onibus. */
	private int id_onibus;
	
	/** The id_localizacao. */
	private String localizacao;
	
	/**
	 * Instantiates a new tracker.
	 */
	public Acompanhador() {
		
	}

	/**
	 * Instantiates a new tracker.
	 *
	 * @param id_onibus the id_onibus
	 * @param id_localizacao the id_localizacao
	 */
	public Acompanhador(int id_onibus, String localizacao) {
		this.id_onibus = id_onibus;
		this.localizacao = localizacao;
	}

	/**
	 * Instantiates a new tracker.
	 *
	 * @param id the id
	 * @param id_onibus the id_onibus
	 * @param id_localizacao the id_localizacao
	 */
	public Acompanhador(int id, int id_onibus, String localizacao) {
		this.id = id;
		this.id_onibus = id_onibus;
		this.localizacao = localizacao;
	}

	/**
	 * Gets the id.
	 *
	 * @return the id
	 */
	public int getId() {
		return id;
	}

	/**
	 * Sets the id.
	 *
	 * @param id the new id
	 */
	public void setId(int id) {
		this.id = id;
	}

	/**
	 * Gets the id_onibus.
	 *
	 * @return the id_onibus
	 */
	public int getId_onibus() {
		return id_onibus;
	}

	/**
	 * Sets the id_onibus.
	 *
	 * @param id_onibus the new id_onibus
	 */
	public void setId_onibus(int id_onibus) {
		this.id_onibus = id_onibus;
	}

	public String getLocalizacao() {
		return localizacao;
	}

	public void setLocalizacao(String localizacao) {
		this.localizacao = localizacao;
	}

}
